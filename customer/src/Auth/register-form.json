          {
            "fieldName": "idNo",
            "fieldLabel": "Omang Number",
            "hint": "123411234",
            "fieldType": "OmangNumber",
            "transformer": [
              {
                "data.ID_NO": {
                  "path": "idType",
                  "default": "PASSPORT",
                  "type": "match",
                  "options": [
                    {
                      "regex": "^\\d{4}[1-2]\\d{4}$",
                      "value": "OMANG"
                    }
                  ]
                }
              },
              {
                "data.SEX": {
                  "path": "sex",
                  "default": "OTHER",
                  "type": "match",
                  "options": [
                    {
                      "regex": "^[M]{1}$",
                      "value": "MALE"
                    },
                    {
                      "regex": "^[F]{1}$",
                      "value": "FEMALE"
                    }
                  ]
                }
              },
              {
                "data.ID_NO": "idNo"
              },
              {
                "data.SURNME": "surname"
              },
              {
                "data.FIRST_NME": "names"
              },
              {
                "data.BIRTH_DTE": "dob"
              },
              {
                "data.EXPIRY_DTE": "exp"
              },
              {
                "data.BIRTH_PLACE_NME": "placeOfBirth"
              }
            ],
            "endpoint_parameter": false,
            "description": "The omang number",
            "tooltip": "",
            "options": "",
            "breakpoints": {
              "xs": 12,
              "sm": 12,
              "md": 6,
