            removeElement('presentContainer')
            setValue('detail4', data.applicationAuthor.idNo)
            setValue('descr4', 'Identity Number')
            setValue('detail5', application.licenseNo)
            setValue('descr5', 'License No.')
            setValue('detail6', data.applicationAuthor.primaryPostalAddress || 'N/A')
