<script>
    const setupS001 = () => {
        if (data.serviceCode == "MTI_007_12_002"){
            setValue('detail1', getCompany().name || 'N/A')
            setValue('descr1', 'Business Name')

            setValidity()
            setConsignmentValidity()
            removeElement('presentContainer')
            setValue('detail4', application.companyRegNo)
            setValue('descr4', 'Business No.')
            setValue('detail5', application.licenseNo)
            setValue('descr5', 'License No.')
            setValue('detail6', formatDate(application.productExportDate))
            setValue('descr6', 'Proposed export Date')
            setValue('detail7', `${data.applicationAuthor.foreNames} ${data.applicationAuthor.lastName}`)
            setValue('descr7', 'Applicant Name')
            // setValue('detail8', application.trailerRegNo)
            // setValue('descr8', 'Trailer Reg. No')
            setValue('detail8', application.exitPort)
            setValue('descr8', 'Exit Port')
            setValue('issuanceDescription', 'Permit Details')

            const products = {
            "product1": "7404: Copper and brass waste and scrap",
            "product2": "7503: Nickel waste and scrap",
            "product3": "7602: Aluminum waste and scrap",
            "product4": "7902: Zinc waste and scrap",
            "product5": "8002: Iron waste and scrap",
            "product6": "8101: Tungsten (Wolfram) and articles thereof including waste and scrap",
            "product7": "8102: Molybdenum and articles thereof including waste and scrap",
            "product8": "8103: Tantalum and articles thereof, including waste and scrap",
            "product9": "8104: Magnesium waste and scrap"
          }

            setValue('tableTitle1', 'Product')
            setValue('subHeader1', 'Description')
            setValue('tableTitle2', '')
            setValue('subHeader2', '')
            setValue('tableTitle4', 'Quantity')
            setValue('subHeader4', 'Tonnes')
            setValue('tableTitle3', 'Vehicle')
            setValue('subHeader3', 'Reg. No.')
            setValue('tableTitle5', 'Destination')
            setValue('subHeader5', 'Country/City')

            let arr = []
            
            data.applicationDetails.exportedGoods.forEach(good=>{
                arr.push({
                    v1: good.product,
                    v2: "",
                    v4: good.quantity,
                    v3: good.vehicleRegNo,
                    v5: good.destination
                })
            })

            const cells = ['h2','h3','h4','h5']
            setWidthByClass('h1', '30%')
            cells.forEach(v=>{
                setWidthByClass(v, '17.5%')
            })

            arr.forEach(a=>{

                const div = document.getElementById('tcp1')
                const clone = div.cloneNode(true);
                clone.id = "foo2";
                clone.style.display = 'flex'
                clone.style.backgroundColor = arr.indexOf(a)%2==1 ? '#DFDFDF76' : '#F9F9F9'
                setElementValue(findInnerChildByClass(clone, 'v1'), a.v1)
                setElementValue(findInnerChildByClass(clone, 'v2'), a.v2)
                setElementValue(findInnerChildByClass(clone, 'v3'), a.v3)
                setElementValue(findInnerChildByClass(clone, 'v4'), a.v4)
                setElementValue(findInnerChildByClass(clone, 'v5'), a.v5)

                document.getElementById("tableChildren").appendChild(clone)
            })
        }
    }
</script>