<script>
    const setupS003 = () => {
        if (data.serviceCode == "MTI_007_12_004"){
            setValidity()
            setConsignmentValidity()
            removeElement('presentContainer')
            setValue('detail1', `${data.applicationAuthor.foreNames} ${data.applicationAuthor.lastName}`)
            setValue('descr1', 'Applicant Name')
            setValue('detail4', data.applicationAuthor.idNo)
            setValue('descr4', 'Identity No.')
            setValue('detail5', data.applicationAuthor.primaryEmailAddress)
            setValue('descr5', "Email")
            setValue('detail6', data.applicationAuthor.primaryPhoneNumber)
            setValue('descr6', 'Phone')
            setValue('detail7', data.applicationAuthor.primaryPostalAddress||'N/A')
